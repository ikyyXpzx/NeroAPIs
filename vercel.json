{
  "version": 2,
  "name": "neroapis",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/css/(.*)",
      "destination": "/public/css/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "APP_NAME": "NeroAPIs",
    "APP_VERSION": "1.0.0"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Application-Name", "value": "NeroAPIs" },
        { "key": "X-Application-Version", "value": "1.0.0" },
        { "key": "X-Powered-By", "value": "NeroAPIs" }
      ]
    }
  ]
}